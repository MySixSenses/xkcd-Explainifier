{
    "name": "xkcd Explanifier",
    "author": "TheUltimateGuide, a Youtube channel",
    "version": "0.0.1.0",
    "manifest_version": 2,
    "description": "A Chromium Extension to open a new tab with the explanified version of a xkcd comic",
    "icons": {
        "16": "icons/nasapod16x16.png",
        "32": "icons/nasapod32x32.png",
        "48": "icons/nasapod48x48.png",
        "128": "icons/nasapod128x128.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["lib/jquery.min.js"]
        }
    ],
    "permissions" : 
    [
        "tabs"
    ]

}
